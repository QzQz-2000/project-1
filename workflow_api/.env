
# .env - 环境配置文件
SERVICE_NAME=workflow-engine
SERVICE_PORT=8000
DEBUG=false

# Kafka配置
KAFKA_SERVERS=localhost:9092
KAFKA_GROUP_ID=workflow-engine

# Redis配置  
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0
REDIS_PASSWORD=

# 日志配置
LOG_LEVEL=INFO
LOG_FORMAT=%(asctime)s - %(name)s - %(levelname)s - %(message)s

# 数据配置
DATA_TTL=3600

# InfluxDB 连接参数
INFLUXDB_URL="http://localhost:8086"
INFLUXDB_TOKEN="mytoken"         # 对应 DOCKER_INFLUXDB_INIT_ADMIN_TOKEN
INFLUXDB_ORG="myorg"             # 对应 DOCKER_INFLUXDB_INIT_ORG
INFLUXDB_BUCKET="mybucket"       # 对应 DOCKER_INFLUXDB_INIT_BUCKET